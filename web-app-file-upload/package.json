{
  "name": "@resty-starter/file-upload",
  "version": "8.1.1",
  "private": true,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typeix/resty-webapp-starter.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build:templates": "",
    "build": "typeix build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "typeix start",
    "start:dev": "typeix start --watch",
    "start:debug": "typeix start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest-runner.json",
    "test:watch": "jest --watch --config ./jest-runner.json",
    "test:cov": "jest --coverage --config ./jest-runner.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.11",
    "@types/handlebars": "^4.1.0",
    "@types/mime": "^2.0.0",
    "coveralls": "^3.1.0",
    "@typeix/cli": "^8.3.0",
    "@typeix/schematics": "^8.3.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "pino-pretty": "^10.0.0",
    "prettier": "^2.8.7",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "ttypescript": "^1.5.15",
    "typescript": "^4.9.5",
    "typescript-transform-paths": "^3.4.6"
  },
  "dependencies": {
    "@typeix/resty": "^8.3.0",
    "@types/busboy": "^0.3.2",
    "busboy": "^0.3.1",
    "fs-extra": "^9.1.0",
    "handlebars": "^4.7.7",
    "mime": "^2.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
