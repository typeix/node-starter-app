{
  "name": "@typeix/resty-starter",
  "version": "8.0.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/typeix/resty-webapp-starter.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build:templates": "",
    "build": "typeix build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "typeix start",
    "start:dev": "typeix start --watch",
    "start:debug": "typeix start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest-runner.json",
    "test:watch": "jest --watch --config ./jest-runner.json",
    "test:cov": "jest --coverage --config ./jest-runner.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "devDependencies": {
    "@typeix/cli": "^8.0.0",
    "@types/fs-extra": "^9.0.11",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^26.0.20",
    "@types/mime": "^2.0.0",
    "@types/node": "^12.19.7",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/eslint-plugin-tslint": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "coveralls": "^3.1.0",
    "eslint": "^7.22.0",
    "jest": "^26.6.3",
    "pino-pretty": "^4.7.1",
    "ts-jest": "^26.5.3",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.4",
    "typescript-transform-paths": "^2.2.3"
  },
  "dependencies": {
    "@typeix/resty": "^8.0.0",
    "@types/busboy": "^0.2.3",
    "busboy": "^0.3.1",
    "fs-extra": "^9.1.0",
    "handlebars": "^4.7.7",
    "mime": "^2.5.2"
  },
  "engines": {
    "node": ">=12.9.0"
  }
}
