{
  "name": "graphql-typeorm-sql",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "migration:run": "typeorm -f src/ormconfig.json migration:run",
    "migration:create": "typeorm -f src/ormconfig.json migration:create -n",
    "prebuild": "rimraf dist",
    "build": "typeix build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "typeix start",
    "start:dev": "typeix start --watch",
    "start:debug": "typeix start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest-runner.json",
    "test:watch": "jest --config ./jest-runner.json --watch",
    "test:cov": "jest --config ./jest-runner.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest-runner.json",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@typeix/resty": "^8.1.1",
    "chalk": "^4.1.1",
    "class-validator": "^0.13.1",
    "highlight": "^0.2.4",
    "pg": "^8.6.0",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.32"
  },
  "devDependencies": {
    "@typeix/cli": "^8.1.1",
    "@typeix/schematics": "^8.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.36",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "pino-pretty": "^4.7.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.5.4",
    "ts-loader": "9.1.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.3",
    "typescript-transform-paths": "^2.2.3"
  },
  "engines": {
    "node": ">=12.9.0"
  }
}
