{
  "name": "graphql-typeorm-sql",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "migration:run": "typeorm -f src/ormconfig.json migration:run",
    "migration:create": "typeorm -f src/ormconfig.json migration:create -n",
    "prebuild": "rimraf dist",
    "build": "typeix build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "typeix start",
    "start:dev": "typeix start --watch",
    "start:debug": "typeix start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest-runner.json",
    "test:watch": "jest --config ./jest-runner.json --watch",
    "test:cov": "jest --config ./jest-runner.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest-runner.json",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@typeix/resty": "^8.6.0",
    "cli-highlight": "^2.1.11",
    "mongodb": "^5.2.0",
    "typeorm": "^0.3.15",
    "chalk": "^4.1.2",
    "highlight": "^0.2.4",
    "pg": "^8.10.0",
    "class-validator": "^0.14.0",
    "graphql": "^15.8.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@typeix/cli": "^8.6.0",
    "@typeix/schematics": "^8.6.0",
    "@types/jest": "^29.5.6",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "pino-pretty": "^10.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "ts-patch": "^3.0.2",
    "typescript": "^5.2.2",
    "typescript-transform-paths": "^3.4.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
