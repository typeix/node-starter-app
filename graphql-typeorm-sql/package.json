{
  "name": "graphql-typeorm-sql",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "migration:run": "typeorm -f src/ormconfig.json migration:run",
    "migration:create": "typeorm -f src/ormconfig.json migration:create -n",
    "prebuild": "rimraf dist",
    "build": "typeix build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "typeix start",
    "start:dev": "typeix start --watch",
    "start:debug": "typeix start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest-runner.json",
    "test:watch": "jest --config ./jest-runner.json --watch",
    "test:cov": "jest --config ./jest-runner.json --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config ./jest-runner.json",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@typeix/resty": "^8.2.3",
    "chalk": "^4.1.1",
    "class-validator": "^0.13.2",
    "graphql": "^15.8.0",
    "highlight": "^0.2.4",
    "pg": "^8.7.3",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.3.3"
  },
  "devDependencies": {
    "@typeix/cli": "^8.2.3",
    "@typeix/schematics": "^8.2.3",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^27.5.1",
    "pino-pretty": "^7.6.0",
    "prettier": "^2.6.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.1",
    "ttypescript": "^1.5.13",
    "typescript": "^4.6.3",
    "typescript-transform-paths": "^3.3.1"
  },
  "engines": {
    "node": ">=16.10.0"
  }
}
